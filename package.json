{
  "name": "vospay-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "flow",
    "test": "yarn lint && yarn typecheck && jest --testPathPattern __tests__ --coverage",
    "postinstall": "flow-typed install",
    "precommit": "yarn lint && yarn typecheck"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "class-autobind": "^0.1.4",
    "dotenv": "^6.1.0",
    "gatsby": "^1.9.255",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-native-web": "^0.1.2",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-resolve-src": "^1.1.4",
    "modal-enhanced-react-native-web": "^0.2.0",
    "mongoose": "^5.3.2",
    "nuka-carousel": "^4.4.3",
    "react": "^16.3.2",
    "react-art": "^16.3.2",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-native": "^0.55.3",
    "react-native-web": "^0.6.1",
    "react-scroll-into-view": "^1.2.0",
    "react-tabs": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-react-native-web": "^0.6.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.74.0",
    "jest": "^23.5.0",
    "prettier": "^1.12.0",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.cache/"
    ]
  }
}
